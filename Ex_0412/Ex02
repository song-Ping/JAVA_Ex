package Ex;

import java.util.Scanner;

public class Ex02 {

	public static void main(String[] args) {
Scanner sc = new Scanner(System.in);
		int a = 5;
		int b = 5;

		if (a > b) {
			System.out.println(">");
		} else if (a < b) {
			System.out.println("<");
		} else if (a == b) {
			System.out.println("==");
		}
	}

}
